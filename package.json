{
  "name": "cv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build",
    "dev": "npm run serve & npm run watch",
    "build": "babel-node build.js > index.html",
    "serve": "http-server",
    "watch": "nodemon -e js,yml --exec 'npm run build'"
  },
  "dependencies": {
    "js-yaml": "^3.10.0",
    "marked": "^0.3.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "http-server": "^0.11.1",
    "nodemon": "^1.17.1"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  }
}
